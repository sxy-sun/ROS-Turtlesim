<launch>
    <node pkg="turtlesim" type="turtlesim_node" name="turtlesim_node" />
    <include file="$(find turtle_driver_ui)/launch/turtle_driver_ui.launch" />
    <include file="$(find turtle_driver)/launch/turtle_driver.launch" />
    
    <node pkg="rviz" type="rviz" name="rviz" />
    <!-- <node pkg="tf" type="static_transform_publisher" name="rviz_broadcaster" args="-4 -4 0 0 0 0 turtle_current_pose_frame turtle_waypoints_frame 100" /> -->
    <node pkg="tf" type="static_transform_publisher" name="rviz_broadcaster" args="-5.5444 -5.5444 0 0 0 0 map turtle_current_pose_frame 100" />

    <rosparam file="$(find system_bringup)/config/params.yaml" />
</launch>